#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/aaliya-ecommerce/backend/node_modules/.pnpm/@medusajs+framework@2.8.6_@medusajs+cli@2.8.6_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4_8fbcea7d91267c8693f38210fe9cf354/node_modules/@medusajs/framework/dist/mikro-orm-cli/node_modules:/workspaces/aaliya-ecommerce/backend/node_modules/.pnpm/@medusajs+framework@2.8.6_@medusajs+cli@2.8.6_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4_8fbcea7d91267c8693f38210fe9cf354/node_modules/@medusajs/framework/dist/node_modules:/workspaces/aaliya-ecommerce/backend/node_modules/.pnpm/@medusajs+framework@2.8.6_@medusajs+cli@2.8.6_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4_8fbcea7d91267c8693f38210fe9cf354/node_modules/@medusajs/framework/node_modules:/workspaces/aaliya-ecommerce/backend/node_modules/.pnpm/@medusajs+framework@2.8.6_@medusajs+cli@2.8.6_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4_8fbcea7d91267c8693f38210fe9cf354/node_modules/@medusajs/node_modules:/workspaces/aaliya-ecommerce/backend/node_modules/.pnpm/@medusajs+framework@2.8.6_@medusajs+cli@2.8.6_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4_8fbcea7d91267c8693f38210fe9cf354/node_modules:/workspaces/aaliya-ecommerce/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/aaliya-ecommerce/backend/node_modules/.pnpm/@medusajs+framework@2.8.6_@medusajs+cli@2.8.6_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4_8fbcea7d91267c8693f38210fe9cf354/node_modules/@medusajs/framework/dist/mikro-orm-cli/node_modules:/workspaces/aaliya-ecommerce/backend/node_modules/.pnpm/@medusajs+framework@2.8.6_@medusajs+cli@2.8.6_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4_8fbcea7d91267c8693f38210fe9cf354/node_modules/@medusajs/framework/dist/node_modules:/workspaces/aaliya-ecommerce/backend/node_modules/.pnpm/@medusajs+framework@2.8.6_@medusajs+cli@2.8.6_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4_8fbcea7d91267c8693f38210fe9cf354/node_modules/@medusajs/framework/node_modules:/workspaces/aaliya-ecommerce/backend/node_modules/.pnpm/@medusajs+framework@2.8.6_@medusajs+cli@2.8.6_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4_8fbcea7d91267c8693f38210fe9cf354/node_modules/@medusajs/node_modules:/workspaces/aaliya-ecommerce/backend/node_modules/.pnpm/@medusajs+framework@2.8.6_@medusajs+cli@2.8.6_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4_8fbcea7d91267c8693f38210fe9cf354/node_modules:/workspaces/aaliya-ecommerce/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/mikro-orm-cli/bin.js" "$@"
else
  exec node  "$basedir/../../dist/mikro-orm-cli/bin.js" "$@"
fi
