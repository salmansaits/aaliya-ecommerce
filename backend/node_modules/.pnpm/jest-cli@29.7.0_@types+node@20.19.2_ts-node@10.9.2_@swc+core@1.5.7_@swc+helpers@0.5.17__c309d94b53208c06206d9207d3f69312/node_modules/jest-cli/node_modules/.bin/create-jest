#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/aaliya-ecommerce/backend/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.19.2_ts-node@10.9.2_@swc+core@1.5.7_@swc+helpers@0.5._40d689963ac12cc40233216c75537279/node_modules/create-jest/bin/node_modules:/workspaces/aaliya-ecommerce/backend/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.19.2_ts-node@10.9.2_@swc+core@1.5.7_@swc+helpers@0.5._40d689963ac12cc40233216c75537279/node_modules/create-jest/node_modules:/workspaces/aaliya-ecommerce/backend/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.19.2_ts-node@10.9.2_@swc+core@1.5.7_@swc+helpers@0.5._40d689963ac12cc40233216c75537279/node_modules:/workspaces/aaliya-ecommerce/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/aaliya-ecommerce/backend/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.19.2_ts-node@10.9.2_@swc+core@1.5.7_@swc+helpers@0.5._40d689963ac12cc40233216c75537279/node_modules/create-jest/bin/node_modules:/workspaces/aaliya-ecommerce/backend/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.19.2_ts-node@10.9.2_@swc+core@1.5.7_@swc+helpers@0.5._40d689963ac12cc40233216c75537279/node_modules/create-jest/node_modules:/workspaces/aaliya-ecommerce/backend/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.19.2_ts-node@10.9.2_@swc+core@1.5.7_@swc+helpers@0.5._40d689963ac12cc40233216c75537279/node_modules:/workspaces/aaliya-ecommerce/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../create-jest@29.7.0_@types+node@20.19.2_ts-node@10.9.2_@swc+core@1.5.7_@swc+helpers@0.5._40d689963ac12cc40233216c75537279/node_modules/create-jest/bin/create-jest.js" "$@"
else
  exec node  "$basedir/../../../../../create-jest@29.7.0_@types+node@20.19.2_ts-node@10.9.2_@swc+core@1.5.7_@swc+helpers@0.5._40d689963ac12cc40233216c75537279/node_modules/create-jest/bin/create-jest.js" "$@"
fi
