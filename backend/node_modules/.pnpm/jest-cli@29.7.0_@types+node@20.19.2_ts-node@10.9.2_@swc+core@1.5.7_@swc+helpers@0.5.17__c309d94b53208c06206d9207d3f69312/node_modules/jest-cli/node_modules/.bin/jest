#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/aaliya-ecommerce/backend/node_modules/.pnpm/jest-cli@29.7.0_@types+node@20.19.2_ts-node@10.9.2_@swc+core@1.5.7_@swc+helpers@0.5.17__c309d94b53208c06206d9207d3f69312/node_modules/jest-cli/bin/node_modules:/workspaces/aaliya-ecommerce/backend/node_modules/.pnpm/jest-cli@29.7.0_@types+node@20.19.2_ts-node@10.9.2_@swc+core@1.5.7_@swc+helpers@0.5.17__c309d94b53208c06206d9207d3f69312/node_modules/jest-cli/node_modules:/workspaces/aaliya-ecommerce/backend/node_modules/.pnpm/jest-cli@29.7.0_@types+node@20.19.2_ts-node@10.9.2_@swc+core@1.5.7_@swc+helpers@0.5.17__c309d94b53208c06206d9207d3f69312/node_modules:/workspaces/aaliya-ecommerce/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/aaliya-ecommerce/backend/node_modules/.pnpm/jest-cli@29.7.0_@types+node@20.19.2_ts-node@10.9.2_@swc+core@1.5.7_@swc+helpers@0.5.17__c309d94b53208c06206d9207d3f69312/node_modules/jest-cli/bin/node_modules:/workspaces/aaliya-ecommerce/backend/node_modules/.pnpm/jest-cli@29.7.0_@types+node@20.19.2_ts-node@10.9.2_@swc+core@1.5.7_@swc+helpers@0.5.17__c309d94b53208c06206d9207d3f69312/node_modules/jest-cli/node_modules:/workspaces/aaliya-ecommerce/backend/node_modules/.pnpm/jest-cli@29.7.0_@types+node@20.19.2_ts-node@10.9.2_@swc+core@1.5.7_@swc+helpers@0.5.17__c309d94b53208c06206d9207d3f69312/node_modules:/workspaces/aaliya-ecommerce/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/jest.js" "$@"
else
  exec node  "$basedir/../../bin/jest.js" "$@"
fi
